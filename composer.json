{
    "name": "neo/neo-hris",
    "description": "Human Resource management system",
    "type": "project",
    "license": "BSD-3-Clause",
    "keywords": [
        "framework",
        "mvc",
        "zf2"
    ],
    "homepage": "http://framework.zend.com/",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^5.6 || ^7.0",
        "zendframework/zend-component-installer": "^1.0 || ^0.3 || ^1.0.0-dev@dev",
        "zendframework/zend-mvc": "^3.0.1",
        "zfcampus/zf-development-mode": "^3.0",
        "zendframework/zend-cache": "^2.7.1",
        "zendframework/zend-db": "^2.8.1",
        "zendframework/zend-mvc-form": "^1.0",
        "zendframework/zend-json": "^3.0",
        "zendframework/zend-log": "^2.9",
        "zendframework/zend-mvc-console": "^1.1",
        "zendframework/zend-mvc-i18n": "^1.0",
        "zendframework/zend-mvc-plugins": "^1.0.1",
        "zendframework/zend-psr7bridge": "^0.2.2",
        "zendframework/zend-session": "^2.7.1",
        "zendframework/zend-servicemanager-di": "^1.0",
        "zf-commons/zfc-twig": "dev-master",
        "zendframework/zend-debug": "^2.5",
        "doctrine/common": ">=2.1",
        "zendframework/zend-navigation": "^2.8",
        "phpoffice/phpexcel": "^1.8",
        "intervention/image": "^2.3",
        "phpixie/image": "^3.2",
        "html2text/html2text": "^4.0",
        "zendframework/zend-mail": "^2.8",
        "phpoffice/phpspreadsheet": "^1.0"
    },
    "autoload": {
        "psr-0": {
            "BjyProfiler": "src/"
        },
        "psr-4": {
            "Application\\": "module/Application/src/",
            "Album\\": "module/Album/src/",
            "LeaveManagement\\": "module/LeaveManagement/src/",
            "HolidayManagement\\": "module/HolidayManagement/src/",
            "Setup\\": "module/Setup/src/",
            "AttendanceManagement\\": "module/AttendanceManagement/src/",
            "SelfService\\": "module/SelfService/src/",
            "RestfulService\\": "module/RestfulService/src/",
            "Payroll\\": "module/Payroll/src/",
            "ManagerService\\": "module/ManagerService/src/",
            "System\\": "module/System/src/",
            "Training\\": "module/Training/src/",
            "Appraisal\\": "module/Appraisal/src/",
            "Loan\\": "module/Loan/src/",
            "Advance\\": "module/Advance/src/",
            "Notification\\": "module/Notification/src/",
            "Travel\\": "module/Travel/src/",
            "WorkOnHoliday\\": "module/WorkOnHoliday/src/",
            "WorkOnDayoff\\": "module/WorkOnDayoff/src/",
            "Overtime\\": "module/Overtime/src/",
            "ServiceQuestion\\": "module/ServiceQuestion/src/",
            "Asset\\": "module/Asset/src/",
            "Other\\": "module/Other/src/",
            "Report\\": "module/Report/src/",
            "MobileApi\\": "module/MobileApi/src/",
            "Customer\\": "module/Customer/src/",
            "Medical\\": "module/Medical/src/",
            "PayrollApi\\": "module/PayrollApi/src/",
            "Cafeteria\\": "module/Cafeteria/src/",
            "KioskApi\\": "module/KioskApi/src/",
            "Cron\\":"module/Cron/src/",
			"PrintLayout\\":"module/PrintLayout/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ApplicationTest\\": "module/Application/test/"
        }
    },
    "extra": [],
    "scripts": {
        "development-disable": "zf-development-mode disable",
        "development-enable": "zf-development-mode enable",
        "development-status": "zf-development-mode status",
        "serve": "php -S 0.0.0.0:8080 -t public/ public/index.php"
    },
    "require-dev": {
        "zendframework/zend-test": "^3.0.1",
        "zendframework/zend-developer-tools": "^1.1"
    },
    "repositories": {
        "packagist": {
            "url": "https://packagist.org",
            "type": "composer"
        }
    }
}
